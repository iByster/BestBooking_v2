    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.12",
    "user-agents": "^1.0.1222",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "xml-js": "^1.6.11",
    "xml-stream": "^0.4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
